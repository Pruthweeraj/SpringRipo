<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<!--BeanPostPeocessor -->
<beans>

	<bean id="triangle" class="com.infinite.bean_post_processor.Triangle">
		<property name="pointA" ref="zeroPoint" />
		<property name="pointB" ref="point1"/>
		<property name="pointC" ref="point2"/>
	</bean>

	<bean id="zeroPoint" class="com.infinite.bean_post_processor.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>

	<bean id="point1" class="com.infinite.bean_post_processor.Point">
		<property name="x" value="20"/>
		<property name="y" value="10"/>
	</bean>

	<bean id="point2" class="com.infinite.bean_post_processor.Point">
		<property name="x" value="-20"/>
		<property name="y" value="5"/>
	</bean>

	<bean class="com.infinite.bean_post_processor.DisplayNameBeanPostProcessor"></bean>
</beans>