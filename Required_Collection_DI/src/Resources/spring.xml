<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
          http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">
	<!-- default category -->

	<bean id="test" class="beans.Test">
		<property name="frouts">
			<util:list list-class="java.util.Vector">
				<value>Apple</value>
				<value>Orange</value>
				<value>Pomogranate</value>
			</util:list>
		</property>

		<property name="languages">
			<util:set set-class="java.util.HashSet">
				<value>C</value>
				<value>Java</value>
				<value>c++</value>
				<value>Spring_Core</value>
			</util:set>
		</property>

		<property name="smartPhones">
			<util:map  map-class="java.util.Hashtable">
				<entry key="Mi" value="13900" />
				<entry key="Nokia" value="14500" />
				<entry key="Samsung" value="14200" />
				<entry key="Apple" value="45000" />
			</util:map>
		</property>

	</bean>







</beans>